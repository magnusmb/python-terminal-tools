"#.*": comment
"(['"]{3})(.|\n)*?\1|(['"]).*?\3": string
"\b([0-9]+\+[0-9]+j|[0-9]*?\.[0-9]+|[0-9]+L?)\b": number
"\b(def|class|from|import|print|assert|return|pass|continue|break)\b": statement
"\b(try|except|while|with|for|in|(el)?if|else|as)\b": statement-block
"\b(True|False|None)\b": keyword
"\b(and|or|all)\b": expression
"\b[a-z_][A-Za-z0-9_]*?(?=\()": function
"\b[A-Z][A-Za-z0-9]*?(?=\()": class
"\b@[a-z]+([a-zA-Z0-9_]*?\w)*?\b": decorator
